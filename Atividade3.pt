 lista.adicionar(4)
 print(lista)  # Saída: "1 2 3 4 "
 lista.remover(2)
 print(lista)  # Saída: "1 3 4 "
                 else:
                     self.ultimo = atual.anterior

                 return True

             atual = atual.proximo

         return False

     def __str__(self):
         lista_str = ""
         atual = self.primeiro

         while atual is not None:
             lista_str += str(atual.valor) + " "
             atual = atual.proximo

         return lista_str
